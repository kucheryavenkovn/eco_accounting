			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	НЕ ДокументРозничнаяПродажа.ОтчетОРозничныхПродажах = ЗНАЧЕНИЕ(Документ.ОтчетОРозничныхПродажах.Пустаяссылка) КАК ЕстьОтчетОРозничныхПродажах,
	ВЫБОР
		КОГДА ДокументРозничнаяПродажа.ПометкаУдаления = ИСТИНА
			ТОГДА 2
		КОГДА ДокументРозничнаяПродажа.Проведен = ИСТИНА
			ТОГДА 1
		ИНАЧЕ 0
	КОНЕЦ КАК СостояниеДокумента,
	ДокументРозничнаяПродажа.ПометкаУдаления,
	ДокументРозничнаяПродажа.Номер,
	ДокументРозничнаяПродажа.Дата,
	ДокументРозничнаяПродажа.ВидОперации,
	ДокументРозничнаяПродажа.Проведен,
	ДокументРозничнаяПродажа.Организация,
	ДокументРозничнаяПродажа.Склад,
