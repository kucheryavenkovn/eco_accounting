	АдресДанныхДляРасчетаПотенциальногоДохода = "";
	ПолученыДанныеДляРасчетаПотенциальногоДохода = Ложь;
	
	ПараметрыПроцедуры = Новый Структура;
	ПараметрыПроцедуры.Вставить("КодВидаДеятельности", КодВидаДеятельностиПСН);
