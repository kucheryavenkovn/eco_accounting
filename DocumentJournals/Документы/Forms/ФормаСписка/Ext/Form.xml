	<Attributes>
		<Attribute name="Список" id="1">
			<Type>
				<v8:Type>cfg:DynamicList</v8:Type>
			</Type>
			<MainAttribute>true</MainAttribute>
			<UseAlways>
				<Field>Список.Ref</Field>
			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ
	ЖурналДокументовДокументы.Ссылка,
	ЖурналДокументовДокументы.Дата,
	ЖурналДокументовДокументы.ПометкаУдаления,
	ЖурналДокументовДокументы.Номер,
	ЖурналДокументовДокументы.Проведен,
	ЖурналДокументовДокументы.Сумма,
	ЖурналДокументовДокументы.Контрагент,
	ЖурналДокументовДокументы.Организация,
	ЖурналДокументовДокументы.Комментарий,
	ВЫБОР
		КОГДА ЖурналДокументовДокументы.Тип = ТИП(Документ.РеализацияТоваровУслуг)
			ТОГДА ВЫБОР
					КОГДА ЖурналДокументовДокументы.УПД
						ТОГДА &amp;ПредставлениеДокументаУПДРеализация
					КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРеализацияТоваров.Услуги)
						ТОГДА &amp;ПредставлениеДокументаАктРеализация
					КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийРеализацияТоваров.Товары)
						ТОГДА &amp;ПредставлениеДокументаНакладнаяРеализация
					ИНАЧЕ &amp;ПредставлениеДокументаРеализация
				КОНЕЦ
		КОГДА ЖурналДокументовДокументы.Тип = ТИП(Документ.ПоступлениеТоваровУслуг)
			ТОГДА ВЫБОР
					КОГДА ЖурналДокументовДокументы.УПД
						ТОГДА
							ВЫБОР 
								КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоступлениеТоваровУслуг.Топливо)
									ТОГДА &amp;ПредставлениеДокументаУПДТопливо
								ИНАЧЕ &amp;ПредставлениеДокументаУПДПоступление
							КОНЕЦ
					КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоступлениеТоваровУслуг.Услуги)
						ТОГДА &amp;ПредставлениеДокументаАктПоступление
					КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоступлениеТоваровУслуг.Товары)
						ТОГДА &amp;ПредставлениеДокументаНакладнаяПоступление
					КОГДА ЖурналДокументовДокументы.ВидОперации = ЗНАЧЕНИЕ(Перечисление.ВидыОперацийПоступлениеТоваровУслуг.Топливо)
						ТОГДА &amp;ПредставлениеДокументаНакладнаяТопливо
					ИНАЧЕ &amp;ПредставлениеДокументаПоступление
				КОНЕЦ
		ИНАЧЕ ПРЕДСТАВЛЕНИЕ(ЖурналДокументовДокументы.Тип)
	КОНЕЦ КАК Тип,
	ВЫБОР
		КОГДА ЖурналДокументовДокументы.Тип = ТИП(Документ.СчетНаОплатуПокупателю)
				ИЛИ ЖурналДокументовДокументы.Тип = ТИП(Документ.СчетНаОплатуПоставщика)
